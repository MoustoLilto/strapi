{
  "kind": "collectionType",
  "collectionName": "cart_items",
  "info": {
    "singularName": "cart-item",
    "pluralName": "cart-items",
    "displayName": "CartItem",
    "description": "Éléments du panier"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "added_at": {
      "type": "datetime",
      "required": true
    },
    "updated_at": {
      "type": "datetime",
      "required": true
    },

    "cart": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cart.cart",
      "inversedBy": "cart_items"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product"
    }
  }
}
